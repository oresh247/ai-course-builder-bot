# Telegram Bot Token
# Получите токен у @BotFather в Telegram
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# OpenAI API Key
# Получите ключ на https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Proxy Settings (optional, для корпоративных сетей)
# Раскомментируйте и настройте, если используете прокси
# HTTP_PROXY=http://your-proxy:port
# HTTPS_PROXY=http://your-proxy:port

# OpenAI API Base URL (optional)
# Используйте, если работаете через прокси для OpenAI
# OPENAI_API_BASE=https://your-proxy-url

